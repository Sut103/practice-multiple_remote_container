{
	"name": "react devcontainer",
	"dockerComposeFile": [
		"../docker-compose.yaml",
		"../.devcontainer/docker-compose.yaml"
	],
	"service": "react",
	"workspaceFolder": "/workspace/frontend",
	"settings": {
		"terminal.integrated.shell.linux": null
	},
	"extensions": [
		"redhat.vscode-yaml"
	],
	"postCreateCommand": "cd app && npm install",
	"shutdownAction": "none"
}